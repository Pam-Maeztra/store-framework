    "props": {
      "text": "Enviamos para todo o Brasil",
      "blockClass": "mz-main-deals__text"
    },
    "title": "Texto - Item 3"
  },

  "flex-layout.col#mz-deals4": {
    "children": ["flex-layout.row#mz-image-deals-4", "rich-text#mz-deal4"],
    "props": {
      "blockClass": "mz-main-deals__block"
    },
    "title": "Item 4"
  },
  "flex-layout.row#mz-image-deals-4": {
    "children": ["image#mz-deal4"],
    "props": {
      "blockClass": "mz-main-deals__image"
    },
    "title": "Imagem - Item 4"
  },
  "image#mz-deal4": {
    "props": {
      "src": "assets/svg/mpa-credit-card.svg",
      "maxHeight": "24px",
      "blockClass": "mz-main-deals__image"
    }
  },
  "rich-text#mz-deal4": {
    "props": {
      "text": "Parcele em até 3x sem juros [saiba mais](#)",
